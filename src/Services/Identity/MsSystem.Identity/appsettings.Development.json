{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ZipkinOptions": {
    "ApplicationName": "IdentityAPI",
    "ZipkinCollectorUrl": "http://localhost:9411"
  },
  "ServiceDiscovery": {
    "Service": {
      "Name": "IdentityAPI",
      "Address": "localhost",
      "Port": 5210,
      "Interval": 10
    },
    "UserServiceName": "IdentityAPI",
    "Consul": {
      "HttpEndpoint": "http://consul:8500",
      "DnsEndpoint": {
        "Address": "localhost",
        "Port": 8600
      }
    }
  }
}
